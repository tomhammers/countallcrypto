  "description": "This project was bootstrapped with [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "index.js",
  "engines": {
    "node": "9.3.0",
    "npm": "5.7.1"
  },
  "scripts": {
    "build-theme": "cd client/src/semantic && gulp build",
    "client": "cd client && npm start",
